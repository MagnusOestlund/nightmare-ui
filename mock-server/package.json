{
  "name": "nightmare-mock-server",
  "version": "6.2.0",
  "description": "Mock API server for Nightmare UI development",
  "scripts": {
    "start": "json-server --watch db.json --port 8001 --routes routes.json --middlewares middleware.js",
    "dev": "json-server --watch db.json --port 8001 --routes routes.json --middlewares middleware.js --delay 500"
  },
  "dependencies": {
    "json-server": "^0.17.4"
  }
}

